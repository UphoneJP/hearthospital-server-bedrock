<% layout('layouts/boilerplate') %>
<div class="row">
    <div class="col-md-6">
        <div class="card mb-2">
            <div class="card-body px-2 px-md-3">
                <h6 class="text-center">
                    リンク集
                </h6>
                <% links.forEach((link,index)=>{ %>
                    <% if(link.category === 'group'){ %>
                        <%- include('linkPartials/linkPartial', {link, index}) %>
                    <% } %>
                <% }) %>
            </div>
        </div>
    </div>

    <div class="col-md-6">
        <div class="card mb-2">
            <div class="card-body">
                <h6 class="text-center">
                    個人SNSへのリンク集
                </h6>
                <% links.forEach((link,index)=>{ %>
                    <% if(link.category === 'personal'){ %>
                        <%- include('linkPartials/linkPartial', {link, index}) %>
                    <% } %>
                <% }) %>
            </div>
        </div>
    </div>
</div>